/* @group Buttons */

/* @group Button Base */

button, a.anchorBtn { 
	position: relative;
	border: 0; 
	padding: 0;
	cursor: pointer;
	overflow: visible; /* removes extra side padding in IE */
	font-family: Arial, sans-serif; 
	text-align: center;
}

a.anchorBtn:hover{ text-decoration: none; }

/* overrides extra padding in Firefox */
button::-moz-focus-inner {
	border: none;  
}

button span,
a.anchorBtn span{ 
	position: relative;
	display: block; 
	white-space: nowrap;	
	color: #444;
	line-height:1;
}

/* Safari and Google Chrome only - fix margins */
@media screen and (-webkit-min-device-pixel-ratio:0) {
	button span {
		margin-top: -1px;
	}
}

