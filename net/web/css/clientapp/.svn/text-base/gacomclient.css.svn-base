	padding: 7px 5px;
	border-bottom: 1px dotted #dfdfdf;
	line-height: 1.4em;
	font-size: 11px;
	color: #666;
	word-wrap: break-word;
}


table.data td p{
	margin: 0;	
}

/* @end Base*/

/* @group THead */

table.data tr.controls th{
	background-color: #fbfbfb;
	padding: 5px;
	vertical-align: bottom;
	border-width: 1px;
	border-style: solid;
	border-color: #DFDFDF #DFDFDF #BFBFBF;
}

table.data form{
	margin: 0;
	padding: 0;
}

table.data .checkBox{
	margin: 0 5px;	
}

table.data input.txtBox{
	margin:3px 0 0 10px;
	padding: 4px;
}

thead .selectWrap{
	height:23px;
	margin:1px 15px 0 0;
	padding:3px 0;
}
/* still digging into this safari rendering issue! #darn-hack */
@media screen and (-webkit-min-device-pixel-ratio:0) {
	table.data thead .selectWrap{		
		position: relative;
		top:2px;
	}
}

table.data label{
	font-weight: normal;
	margin: 0 7px 0 0;
	color: #666;
	font-size: 11px;
}

table.data .selectBox{
	margin: 0;
	width: 100px;
	font-size: 11px;	
}

table.data .selectBox.medium{
	width: 180px;
}

table.data .selectBox.long{
	width: 240px;	
}


table.data .threeFilters .selectBox{  
	width: 105px;
}

table.data .threeFilters .selectWrap{ 
	margin-right: 15px;
}

table.data .omega,
table.data .threeFilters .omega{
	margin-right: 0;
}


table.data h5{
	padding: 7px 0 0 5px;
	color:#1c1c1c;
	line-height: 1;
}

table.data tr.controls .actions{
	margin:5px 0 0 5px;
	padding:5px 0;
}

table.data tr.controls .actions a{
	font-weight: normal;
	margin: 0 5px;
}

table.data tr.controls .actions a.alpha{
	margin-left: 0;
}

table.data tr.controls .actions a.omega{
	margin-right: 0;
}

table.data tr.controls .actions .label{
	font-size: 11px;
	font-weight: normal;
	margin-right: 10px;
	color: #666;
}

.separator{
	font-size: 11px;
	color: #666;
	margin: 0 7px;
}

.grayedOut {
	background-color: #fbfbfb;
}

table.data tr.dataHeader th.alpha{
	border-left: 1px solid #dfdfdf;
}

table.data tr.dataHeader th.omega{
	border-right: 1px solid #dfdfdf;
}

table.data td .actions{ padding: 0; }

table.data tr.dataHeader th{
	background-color:#f0f0f0;
	height: 36px;
	vertical-align:middle;
	padding: 0 5px;
	border-bottom: 1px solid #dfdfdf;
}

/* @end THead*/


/* @group TFoot */

table.data .tFoot td{
	padding: 0;
