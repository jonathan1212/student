/* @group Status Background */

.errorBackground{
	background-color: #ffe2e6;
}

.noticeBackground{
	background-color: #ffffe1;
}

.successBackground{
	background-color: #e8fde0;
}

/* @end Status Background */

.dataMeter{
	margin: 0;
}

.notice .head{ 
	padding-bottom: 5px;
}

