input.txtBox,
textarea.txtArea{ 
	border: 1px solid #bfbfbf;
}


input.txtBox:focus,
.txtArea:focus{
	border-color: #666;
}

form .radio .details,
form .checkBox .details{ margin-top: 3px; }

form .radio .indent,
form .checkBox .indent,
form .check .indent{ padding-left: 20px; }

form .item .indentedItem{ padding-top: 7px; padding-bottom: 10px;}

form .radio, 
form .check{
	font-size: 12px;
	margin: 5px 0;
}

form .radio label, 
form .check label{
	font-weight: normal;
	margin: 0;
	font-size: 11px;
}


form .radioBtn, 
form .checkBox{
	position: relative;
	top: 0px;
	margin-left: 0;
	margin-bottom: 0;
}

form .actions{
	padding: 10px 0px;
}

textarea{
	line-height: 1.6em;
	height: 200px;
}

/* @end Forms */

