/* @group Buttons */

/* @group Button Base */

button, a.anchorBtn { 
	position: relative;
	border: 0; 
	padding: 0;
	cursor: pointer;
	overflow: visible; /* removes extra side padding in IE */
	font-family: Arial, sans-serif; 
	text-align: center;
}

/* overrides extra padding in Firefox */
button::-moz-focus-inner {
	border: none;  
}

button span,
a.anchorBtn span{ 
	position: relative;
	display: block; 
	white-space: nowrap;	
	color: #000;
	line-height:1;
}

/* Safari and Google Chrome only - fix margins */
@media screen and (-webkit-min-device-pixel-ratio:0) {
	button span {
		margin-top: -1px;
	}
}

button.large.emphasize span, 
button.small.emphasize span,
a.anchorBtn.large.emphasize span,
a.anchorBtn.small.emphasize span{
	color: #0072bc;
}

button.large.emphasize:hover span,
button.small.emphasize:hover span,
a.anchorBtn.large.emphasize:hover span,
a.anchorBtn.small.emphasize:hover span{
	color: #288ACA;
}

/* @end Button Base*/

/* @group Small Button Skin */

/* @group Base */

button.small,
a.anchorBtn.small{
	font-size:11px;
	font-weight:normal;
	margin-right:2px;
	padding:0 10px 0 0;
}

button.small span,
a.anchorBtn.small span{
	height:12px;
	padding:9px 0 9px 10px;
}

/* the redundant class is used to apply the hover state with a script */
button.small:hover,
a.anchorBtn.small:hover,
button.smallHover { 
	background-position: right -182px; 
	color: #666;
}
	
button.small:hover span,
a.anchorBtn.small:hover span, 
button.smallHover span {
	background-position: 0 -56px;
	color: #666;
}

button.small:active,
a.anchorBtn.small:active,
button.smallActive,
button.large:active,
a.anchorBtn.large:active,
button.largeActive {  
	position: relative;
	top: 1px;
}
	

/* @end Base*/

/* @group Blue */

button.blueSmall,
a.anchorBtn.blueSmall{  
	background: transparent url(/images/clientapp/button_liteblue_small.gif) no-repeat scroll right -110px;
}

button.blueSmall span,
a.anchorBtn.blueSmall span {
	background: transparent url(/images/clientapp/button_liteblue_small.gif) no-repeat left top; 
}

/* @end Blue*/

/* @group Black */

button.blackSmall,
a.anchorBtn.blackSmall{  
	background: transparent url(/images/clientapp/button_black_small.gif) no-repeat scroll right -110px;
}

button.blackSmall span,
a.anchorBtn.blackSmall span {
	background: transparent url(/images/clientapp/button_black_small.gif) no-repeat left top; 
}

/* @end Black*/

/* @group White */

