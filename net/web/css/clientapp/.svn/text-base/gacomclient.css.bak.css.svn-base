}

em, dfn, .em {
	font-style: italic;	
}

dfn{
	font-weight: bold;
}

label{
	font-size: 12px;
	font-weight: bold;
	margin: 5px 0 10px;
}

strong, .strong{
	font-weight: bold;
}

address{
	font-style: normal;
	font-size: 12px;
	line-height: 1.5em;
}

