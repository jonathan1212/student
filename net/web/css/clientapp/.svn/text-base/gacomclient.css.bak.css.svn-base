a.anchorBtn.whiteSmall{  
	background: transparent url(/images/clientapp/button_white_small.gif) no-repeat scroll right -110px;
}

button.whiteSmall span,
a.anchorBtn.whiteSmall span {
	background: transparent url(/images/clientapp/button_white_small.gif) no-repeat left top; 
}

/* @end Whtite*/


/* @end Small Button Skin*/


/* @group Large Button Skin */

/* @group Base */

button.large,
a.anchorBtn.large{
	padding: 0 12px 0 0; 
	margin-right:5px;
	font-size:12px; 
	font-weight: bold;
}

button.large span,
a.anchorBtn.large span{
	left: -2px;
	height:14px;
	padding:12px 0 10px 15px;
}

